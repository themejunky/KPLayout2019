<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:latin="http://schemas.android.com/apk/res-auto">

    <include
        latin:keyboardLayout="@xml/key_styles_common" />

    <Row latin:keyWidth="10%p" latin:rowHeight="15.5%p">
        <include latin:keyboardLayout="@xml/rowkeys_symbols1"/>
    </Row>

    <Row latin:keyWidth="10%p" latin:rowHeight="21%p">
        <include latin:keyboardLayout="@xml/rowkeys_qwerty1" />
    </Row>

    <Row latin:keyWidth="10%p" latin:rowHeight="21%p">
        <include latin:keyboardLayout="@xml/rowkeys_qwerty2" latin:keyXPos="5%p" />
    </Row>

    <Row latin:keyWidth="10%p" latin:rowHeight="21%p">
        <Key latin:keyStyle="shiftKeyStyle" latin:keyWidth="15%p" />
        <!--latin:visualInsetsRight="1%p"-->
        <include latin:keyboardLayout="@xml/rowkeys_qwerty3" />
        <Key latin:keyStyle="deleteKeyStyle" latin:keyWidth="fillRight"/>
        <!--latin:visualInsetsLeft="1%p"-->
    </Row>
    <include latin:keyboardLayout="@xml/row_qwerty4" />
</merge>
